{ "version": "1.8"
, "manifest_version": 2
, "name": "Gravatar Zoom"
, "homepage_url": "https://github.com/johan/gravatar-zoom/"
, "description": "Hover gravatar images anywhere on the web to zoom them up."
, "icons":
  { "16": "16.png"
  , "48": "48.png"
  , "128":"128.png"
  }
, "content_scripts":
  [ { "matches":
      [ "http://*/*"
      , "https://*/*"
      , "file:///*"
      ]
    , "js":
      [ "jquery-1.7.2.min.js"
      , "gravatar-zoom.user.js"
      ]
    , "all_frames": true
    }
  ]
}
